{
  "name": "sm-2",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.9.0",
  "description": "A TypeScript implementation of the SuperMemo 2 (SM2) algorithm for spaced based repetition algorithm",
  "author": "Kirk Lin <linkirk@163.com> (https://github.com/kirklin)",
  "license": "MIT",
  "keywords": [
    "sm2",
    "supermemo",
    "flashcards",
    "supermemo-2",
    "spaced-repetition",
    "javascript",
    "typescript"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.18.0",
    "@antfu/utils": "^0.5.2",
    "@kirklin/eslint-config": "^0.1.2",
    "@types/node": "^18.7.15",
    "bumpp": "^8.2.1",
    "eslint": "^8.22.0",
    "esno": "^0.16.3",
    "pnpm": "^7.9.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.8.8",
    "vite": "^3.0.7",
    "vitest": "^0.22.0"
  }
}
